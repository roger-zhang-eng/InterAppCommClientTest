# Uncomment this line to define a global platform for your project
platform :ios, '8.0'

target 'OneFillSim' do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for OneFillSim
    pod 'Alamofire', '~> 3.4' # HTTP networking library written in Swift - https://github.com/Alamofire/Alamofire
    pod 'SwiftyJSON', '~> 2.3.2' #JSON parsing by Swift - https://github.com/SwiftyJSON/SwiftyJSON
    pod 'InterAppCommunication', '~> 1.0'  #Inter App Communication by ObjC - https://github.com/tapsandswipes/InterAppCommunication

end

target 'OneFillSimTests' do
    #inherit! :search_paths
    # Pods for testing
end
